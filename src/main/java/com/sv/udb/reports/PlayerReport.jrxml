	<queryString>
		<![CDATA[SELECT
     jugadores.`codi_juga` AS jugadores_codi_juga,
     jugadores.`nomb_juga` AS jugadores_nomb_juga,
     jugadores.`edad_juga` AS jugadores_edad_juga,
